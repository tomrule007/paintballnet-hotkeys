{
  "name": "paintballnet-hotkeys",
  "version": "1.1.0",
  "description": "Bookmarklet that allows users to create hotkeys for paintballnet.net",
  "main": "app.js",
  "scripts": {
    "start": "webpack-dev-server --config webpack.dev.js --open",
    "build": "webpack --config webpack.prod.js",
    "bookmark": "bookmarklet -d ./dist/main.js ./build/index.html && date",
    "dev": "concurrently \"webpack --watch\" \"watch 'npm run bookmark' dist\" \"live-server build/index.html\"",
    "build2": "webpack && npm run bookmark",
    "test": "jest"
  },
  "keywords": [
    "paintballnet",
    "hotkey",
    "bookmarklet"
  ],
  "author": "Thomas Herzog",
  "license": "ISC",
  "dependencies": {
    "bookmarklet": "^1.0.4"
  },
  "devDependencies": {
    "@babel/cli": "^7.10.5",
    "@babel/core": "^7.11.4",
    "@babel/plugin-proposal-class-properties": "^7.10.4",
    "@babel/preset-env": "^7.11.0",
    "@testing-library/dom": "^7.22.6",
    "@testing-library/jest-dom": "^5.11.4",
    "babel-jest": "^26.3.0",
    "babel-loader": "^8.1.0",
    "concurrently": "^5.3.0",
    "html-webpack-plugin": "^4.4.1",
    "jest": "^26.4.2",
    "live-server": "^1.2.1",
    "terser-webpack-plugin": "^4.1.0",
    "watch": "^1.0.2",
    "webpack": "^4.44.1",
    "webpack-cli": "^3.3.12",
    "webpack-dev-server": "^3.11.0",
    "webpack-merge": "^5.1.3"
  }
}
