{
  "name": "paintballnet-hotkeys",
  "version": "1.2.0",
  "description": "Bookmarklet that allows users to create hotkeys for paintballnet.net",
  "main": "app.js",
  "scripts": {
    "start": "webpack-dev-server --config webpack.dev.js --open",
    "build": "webpack --config webpack.prod.js",
    "test": "jest",
    "coverage": "jest --coverage --collectCoverageFrom='src/**/*.js'",
    "coverageReport": "jest --coverage --coverageDirectory='coverage' --collectCoverageFrom='src/**/*.js'",
    "lint": "eslint src/*.js"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "*.js": [
      "eslint --fix",
      "git add"
    ]
  },
  "keywords": [
    "paintballnet",
    "hotkey",
    "bookmarklet"
  ],
  "author": "Thomas Herzog",
  "license": "ISC",
  "dependencies": {
    "bookmarklet": "^1.0.4"
  },
  "devDependencies": {
    "@babel/cli": "^7.10.5",
    "@babel/core": "^7.11.4",
    "@babel/preset-env": "^7.11.0",
    "@testing-library/dom": "^7.22.6",
    "@testing-library/jest-dom": "^5.11.4",
    "babel-jest": "^26.3.0",
    "babel-loader": "^8.1.0",
    "css-loader": "^4.2.2",
    "eslint": "^7.8.1",
    "eslint-config-prettier": "^6.11.0",
    "eslint-config-standard": "^14.1.1",
    "eslint-plugin-import": "^2.22.0",
    "eslint-plugin-node": "^11.1.0",
    "eslint-plugin-prettier": "^3.1.4",
    "eslint-plugin-promise": "^4.2.1",
    "eslint-plugin-standard": "^4.0.1",
    "html-webpack-plugin": "^4.4.1",
    "husky": "^4.2.5",
    "jest": "^26.4.2",
    "prettier": "2.1.1",
    "schema-utils": "^2.7.1",
    "style-loader": "^1.2.1",
    "webpack": "^4.44.1",
    "webpack-cli": "^3.3.12",
    "webpack-dev-server": "^3.11.0",
    "webpack-merge": "^5.1.3",
    "webpack-sources": "^1.4.3"
  }
}
